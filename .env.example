# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

SITE_NAME="Silicon Flow"

# change in production
NEXT_PUBLIC_DOMAIN="http://localhost:3000"
NEXT_PUBLIC_API_DOMAIN="https://cloud.siliconflow.com"
# NEXT_PUBLIC_API_DOMAIN="https://rest.siliconflow.com"
NEXT_PUBLIC_SDK_DOMAIN="https://api.siliconflow.com"

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings
DATABASE_URL=
SHADOW_DATABASE_URL=

# enable in production
# https://github.com/chrishoermann/zod-prisma-types?tab=readme-ov-file#skip-schema-generation
SKIP_ZOD_PRISMA = "false"

# openssl rand -hex 32
AUTH_SECRET=

# github.com developer settings, set up a new oauth app.
# see http://localhost:3000/api/auth/providers to get the callback url
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# smtp settings for sending emails by nodemailer. setup hotmail email smtp settings
SMTP_USER=
SMTP_PASSWORD=
SMTP_HOST=smtp-mail.outlook.com
SMTP_PORT=587
EMAIL_FROM=
